- @page_header = "Sign In"
.row
  .col-md-5.col-md-offset-1
    = form_tag do
      .form-group
        = label_tag "Email Address"
        = email_field_tag "Email Address", nil, class: "form-control"
      .form-group
        = label_tag "Password"
        = password_field_tag "Password", nil, class: "form-control"
      = submit_tag "Sign In", class: "btn btn-primary"
